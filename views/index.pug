extends ./layouts/common.pug

block carousel
    #carouselExampleControls.carousel.slide(data-ride='carousel')
        .carousel-inner
            .carousel-item.active
                img.d-block.w-100(src='images/greenwich4.jpg', alt='First slide')
            .carousel-item
                img.d-block.w-100(src='images/greenwich1.png', alt='Second slide')
            .carousel-item
                img.d-block.w-100(src='images/greenwich6.jpg', alt='Third slide')

